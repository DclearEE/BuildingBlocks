{
  "$schema": "https://hypar.io/schemas/function.json",
  "description": "Adds Masses representing rooms and corridors to one or more Masses named 'envelope'.",
  "id": "b4105629-97ae-4419-b717-0cecd166297d",
  "name": "Story",
  "model_dependencies": ["envelope"],
  "model_output": "story",
  "repository_url": "https://github.com/hypar-io/function",
  "inputs": [
    {
      "min": 3.0,
      "max": 5.0,
      "step": 0.5,
      "name": "Story height",
      "description": "Typical story height.",
      "type": "range"
    },
    {
      "min": 0.1,
      "max": 1.0,
      "step": 0.1,
      "name": "Envelope offset",
      "description": "Offset from the enclosing envelope of the story.",
      "type": "range"
    },
    {
      "min": 2.0,
      "max": 20.0,
      "step": 2.0,
      "name": "Rooms Per Story",
      "description": "Quantity of rooms on each floor.",
      "type": "range"
    },
    {
      "min": 2.0,
      "max": 4.0,
      "step": 0.5,
      "name": "Corridor Width",
      "description": "Corridor width.",
      "type": "range"
    }
  ],
  "outputs": [
    {
      "name": "Story count",
      "description": "Story count.",
      "type": "number"
    },
    {
      "name": "Room Quantity",
      "description": "Room quantity.",
      "type": "number"
    }
  ]
}